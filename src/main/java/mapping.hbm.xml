<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.practiceDemo.entity.Student" table="student">
		<id name="rollNo" column="roll_no"></id>
		<property name="studName" column="stude_name"></property>
		<property name="studAdd" column="stude_add"></property>
	</class>
</hibernate-mapping>

